pipeline{
  agent any
  tools { 
      maven 'M2_HOME' 
    } 
  stages{
     stage("git checkout"){
         steps{
            git branch: 'main', url: 'https://github.com/Sudhivya/demo-counter-app.git'
          }
     }
     stage("unit testing"){
         steps{
            sh 'mvn test'
         }
      }
  }
}
