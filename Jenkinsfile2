pipeline{
    agent any
    tools { 
      maven 'M2_HOME' 
      jdk 'JAVA_HOME' 
    }
    stages{
       stage('Git checkout'){
          steps{
              git branch: 'main', url: 'https://github.com/Sudhivya/demo-counter-app.git'
          }
       }
       stage('integration test'){
          steps{
              sh 'mvn verify -DskiUnitTests'
          }
       }
    }
}
